      </items>
    </status><status>
      <id>deleted</id>
      <name>Supprim&#233;e</name>
      <colour>FFFFFF</colour>
      <visibility />
      <items>
        <item id="_remove" type="remove" />
      </items>
    </status>
  </possible_status>
</workflow>