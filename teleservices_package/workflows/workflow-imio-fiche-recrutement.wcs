          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel ouvrier"</value>
          </condition>
        </item><item id="2" type="commentable">
          <label>Commentaire</label>
          <button_label>Ajouter le commentaire</button_label>
          <by>
            <item role_id="_responsable-recrutement-personnel-social">_responsable-recrutement-personnel-social</item>
          </by><required>False</required>
          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel social"</value>
          </condition>
        </item><item id="3" type="commentable">
          <label>Commentaire</label>
          <button_label>Ajouter le commentaire</button_label>
          <by>
            <item role_id="_responsable-recrutement-personnel-technique-specifique">_responsable-recrutement-personnel-technique-specifique</item>
          </by><required>False</required>
          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel technique / sp&#233;cifique"</value>
          </condition>
        </item><item id="4" type="commentable">
          <label>Commentaire</label>
          <button_label>Ajouter le commentaire</button_label>
          <by>
            <item role_id="_responsable-recrutement-personnel-de-nettoyage">_responsable-recrutement-personnel-de-nettoyage</item>
          </by><required>False</required>
          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel de nettoyage"</value>
          </condition>
        </item><item id="5" type="commentable">
          <label>Commentaire</label>
          <button_label>Ajouter le commentaire</button_label>
          <by>
            <item role_id="_responsable-recrutement-personnel-de-cuisine">_responsable-recrutement-personnel-de-cuisine</item>
          </by><required>False</required>
          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel de cuisine"</value>
          </condition>
        </item><item id="6" type="commentable">
          <label>Commentaire</label>
          <button_label>Ajouter le commentaire</button_label>
          <by>
            <item role_id="_responsable-recrutement-personnel-du-service-enseignement">_responsable-recrutement-personnel-du-service-enseignement</item>
          </by><required>False</required>
          <condition>
            <type>django</type>
            <value>form_var_section == "Personnel du service enseignement"</value>
          </condition>
        </item><item id="_action_delete" type="choice">
