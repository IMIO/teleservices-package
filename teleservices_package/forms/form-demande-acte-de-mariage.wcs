      </condition><display_locations />
      <id type="str">92</id>
    </field><field>
      <label type="str">Extrait/Copie int&#233;grale d'acte de Mariage</label>
      <type type="str">page</type>
      <id type="str">1</id>
    </field><field>
      <label type="str">BAEC cat&#233;gorie</label>
